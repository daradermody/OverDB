{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "script"
  },
  "ecmaFeatures": {
    "modules": true,
    "spread": true,
    "restParams": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "max-len": ["error", {"code": 120}],
    "require-jsdoc": "off"
  }
}
