{
  "name": "@overdb/frontend",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "dev": "node scripts/devServer.js",
    "build": "rm -rf build/* && yarn graphql-type-gen && esbuild --bundle src/index.tsx --outdir=build/static --minify --define:SERVER_URL=\\\"https://dzbi8yrdpm1hx.cloudfront.net\\\" && rsync -a public/* build --exclude static",
    "graphql-type-gen": "graphql-codegen --config codegen.yml",
    "deploy": "aws s3 sync build s3://serverless-website-dara --exact-timestamps --delete && aws cloudfront create-invalidation --distribution-id E1UPTX47GL3PH1 --paths '/*'"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "^5.0.0-alpha.101",
    "@mui/material": "^5.10.7",
    "@mui/styles": "^5.10.7",
    "@preact/signals": "^1.1.1",
    "@react-hook/throttle": "^2.2.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "esbuild": "^0.15.9",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "preact": "^10.11.0",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.1",
    "@graphql-codegen/fragment-matcher": "3.3.1",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@overdb/backend": "workspace:*",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.7.23",
    "esbuild-server": "^0.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
