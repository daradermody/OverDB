    "@babel/preset-env": "^7.13.15",
    "@types/deepmerge": "^2.1.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^15.0.1",
    "@types/nodemailer": "^4.6.0",
    "@types/react-router-dom": "^5.1.7",
    "babel-loader": "^8.2.2",
    "codelyzer": "^4.3.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
