{
  "name": "@overdb/backend",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "start": "ts-node src",
    "dev": "nodemon",
    "graphql-type-gen": "graphql-codegen --config codegen.yml",
    "deploy": "rsync --archive --ignore-existing --recursive --rsh 'ssh -i ~/.ssh/overdb.pem' . ubuntu@ec2-63-32-108-2.eu-west-1.compute.amazonaws.com:/home/ubuntu/OverDB-server",
    "ssh": "ssh -i ~/.ssh/overdb.pem ubuntu@ec2-63-32-108-2.eu-west-1.compute.amazonaws.com"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.6.0",
    "jsdom": "^20.0.0",
    "moviedb-promise": "^3.1.18",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.12.1",
    "@graphql-codegen/fragment-matcher": "3.3.1",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@graphql-codegen/typescript-react-apollo": "3.3.3",
    "@graphql-codegen/typescript-resolvers": "2.7.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/jsdom": "^20.0.0",
    "esbuild": "^0.15.8",
    "nexe": "^4.0.0-rc.1",
    "nodemon": "^2.0.20",
    "typescript": "^4.8.3"
  },
  "nodemonConfig": {
    "exec": "yarn start",
    "ext": "ts",
    "ignore": [
      "database/user_data.json",
      "moviedb_cache.json"
    ]
  }
}
