{
  "name": "over-db",
  "version": "0.1.0",
  "author": "Dara Dermody",
  "description": "Web application that allows people to easily follow their favourite cast and crew in movies",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daradermody/OverDB.git"
  },
  "keywords": [
    "movies",
    "director",
    "actor",
    "application"
  ],
  "bugs": {
    "url": "https://github.com/daradermody/OverDB/issues"
  },
  "scripts": {
    "dev": "concurrently -n client,server -c green,cyan 'yarn workspace @overdb/frontend dev' 'yarn workspace @overdb/backend dev'"
  },
  "nodemonConfig": {
    "exec": "npx ts-node server",
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "server/database/user_data.json",
      "server/moviedb_cache.json"
    ]
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/lab": "^5.0.0-alpha.93",
    "@mui/material": "^5.9.3",
    "@mui/styles": "^5.9.3",
    "@react-hook/throttle": "^2.2.0",
    "@saeris/apollo-server-vercel": "^1.0.1",
    "@types/lodash": "^4.14.182",
    "@types/lodash.throttle": "^4.1.7",
    "apollo-server": "^3.10.0",
    "apollo-server-core": "^3.10.0",
    "apollo-server-express": "^3.10.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "concurrently": "^7.3.0",
    "core-js": "^3.24.1",
    "cors": "^2.8.5",
    "esbuild": "^0.14.53",
    "express": "^4.18.1",
    "graphql": "15",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "moviedb-promise": "^3.1.15",
    "nodemailer": "^6.7.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@graphql-codegen/cli": "2.11.3",
    "@graphql-codegen/fragment-matcher": "3.3.0",
    "@graphql-codegen/introspection": "2.2.0",
    "@graphql-codegen/typescript": "2.7.2",
    "@graphql-codegen/typescript-operations": "2.5.2",
    "@graphql-codegen/typescript-react-apollo": "3.3.2",
    "@graphql-codegen/typescript-resolvers": "2.7.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.4",
    "@types/nodemailer": "^6.4.5",
    "@types/react": "^18.0.15",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
