{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react",
    "target": "es6",
    "baseUrl": ".",
    "lib": ["es2017", "dom"],
  },
  "exclude": [
    "node_modules",
    "server/lib_types/**"
  ]
}
