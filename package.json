{
  "name": "over-db",
  "version": "0.1.0",
  "author": "Dara Dermody",
  "description": "Web application that allows people to easily follow their favourite cast and crew in movies",
  "main": "server.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daradermody/OverDB.git"
  },
  "keywords": [
    "movies",
    "director",
    "actor",
    "application"
  ],
  "bugs": {
    "url": "https://github.com/daradermody/OverDB/issues"
  },
  "scripts": {
    "test": "eslint . && ./list_unused_css_classes.sh && stylelint '**/*.css' '**/*.html' && ng lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "server/database/data_files/"
    ],
    "watch": [
      "server"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/http": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.1.0",
    "core-js": "^2.5.5",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "rxjs": "^5.5.10",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.10",
    "@angular/language-service": "^5.2.10",
    "@types/node": "^6.0.106",
    "codelyzer": "^4.3.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "husky": "^0.14.3",
    "nodemon": "^1.17.3",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
