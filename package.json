{
  "name": "over-db",
  "version": "0.1.0",
  "author": "Dara Dermody",
  "description": "Web application that allows people to easily follow their favourite cast and crew in movies",
  "main": "server/index.ts",
  "license": "MIT",
  "packageManager": "yarn@3.1.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daradermody/OverDB.git"
  },
  "keywords": [
    "movies",
    "director",
    "actor",
    "application"
  ],
  "bugs": {
    "url": "https://github.com/daradermody/OverDB/issues"
  },
  "scripts": {
    "server": "nodemon",
    "start": "webpack serve --mode development",
    "build": "webpack build --mode production",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test"
    }
  },
  "nodemonConfig": {
    "exec": "npx ts-node server",
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "server/database/user_data.json",
      "server/moviedb_cache.json"
    ]
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/lab": "^5.0.0-alpha.59",
    "@mui/material": "^5.2.3",
    "@mui/styles": "^5.2.3",
    "@react-hook/throttle": "^2.2.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash.throttle": "^4.1.6",
    "apollo-server": "^3.5.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "core-js": "^3.19.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^16.1.0",
    "jsdom": "^19.0.0",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "morgan": "^1.10.0",
    "moviedb-promise": "^3.1.12",
    "nodemailer": "^6.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/fragment-matcher": "3.2.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@types/jsdom": "^16.2.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "^17.0.37",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.2.3",
    "eslint": "^8.4.1",
    "eslint-config-google": "^0.14.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
